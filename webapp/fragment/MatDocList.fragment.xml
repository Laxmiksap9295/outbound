<core:FragmentDefinition xmlns="sap.m" xmlns:l="sap.ui.layout" xmlns:m="sap.ui.commons.layout" xmlns:f="sap.ui.layout.form"
	xmlns:core="sap.ui.core">
	<Dialog stretchOnPhone="true" type="Standard">
		<customHeader>
			<Bar class="Inbound_Bar1">
				<contentMiddle>
					<Label text="{i18n>Title12}" class="Inbound_Title" design="Bold"/>
				</contentMiddle>
			</Bar>
		</customHeader>
		<content>
			<List id="listId" growing="false" items="{MatDocList>/}">
				<CustomListItem class="clCustomListItem sapUiTinyMarginTop sapUiTinyMarginBegin text1Rem" type="Active">
					<Toolbar>
						<Vbox>
							<Text text="{i18n>Pmblnr}" class="cl_DocText"/>
							<Text text="{MatDocList>Pmblnr}" class="cl_DocTextval"/>
						</Vbox>
						<ToolbarSpacer/>
						<VBox>
							<Text text="{i18n>DocItem}" class="cl_DocText"/>
							<Text text="{MatDocList>Zeile}" class="cl_DocTextval"/>
						</VBox>
					</Toolbar>
					<Toolbar class="sapUiTinyMarginTop">
						<Vbox>
							<Text text="{i18n>Item}" class="cl_DocText"/>
							<Text text="{MatDocList>Item}" class="cl_DocTextval"/>
						</Vbox>
						<ToolbarSpacer/>
						<VBox>
							<Text text="{i18n>Material}" class="cl_DocText"/>
							<Text text="{path:'MatDocList>Matnr',formatter:'LoadingConfirmation.model.formatter.LeadingZero'}" class="cl_DocTextval"/>
						</VBox>
					</Toolbar>
					<!--<Toolbar class="sapUiTinyMarginTop">-->
					<!--	<Vbox>-->
					<!--		<Text text="{i18n>Remarks}" class="cl_DocText"/>-->
					<!--		<Text text="{MatDocList>Remarks}" class="cl_DocTextval"/>-->
					<!--	</Vbox>-->
					<!--	<ToolbarSpacer/>-->
					<!--</Toolbar>-->
				</CustomListItem>
			</List>
		</content>
		<endButton>
			<Button press="onCloseMatDoc" text="{i18n>Label17}"></Button>
		</endButton>
	</Dialog>
</core:FragmentDefinition>